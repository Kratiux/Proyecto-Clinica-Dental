{

    "version": 2,
    "builds": [
        {"src": "*.js", "use":"@vercel/node"}
    ],

    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],

    "rewrite": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]

}